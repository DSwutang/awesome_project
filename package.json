{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest --coverage --testResultsProcessor=jest-sonar-reporter --silent --watchAll=false",
    "lint": "eslint --fix --ext ."
  },
  "dependencies": {
    "@ant-design/react-native": "^4.0.5",
    "@expo/vector-icons": "^10.2.1",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/cameraroll": "^4.0.1",
    "@react-native-community/datetimepicker": "^3.0.5",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-community/picker": "^1.8.1",
    "@react-native-community/segmented-control": "^2.2.1",
    "@react-native-community/slider": "^3.0.3",
    "@react-native-community/viewpager": "^4.2.0",
    "@react-navigation/bottom-tabs": "^5.10.0",
    "@react-navigation/native": "^5.7.6",
    "@react-navigation/stack": "^5.9.3",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^26.5.2",
    "babel-preset-react-native": "^1.9.0",
    "deprecated-react-native-listview": "0.0.6",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "jest": "^26.5.2",
    "jest-sonar-reporter": "^2.0.0",
    "react": "16.13.1",
    "react-native": "0.63.3",
    "react-native-camera": "^3.40.0",
    "react-native-fs": "^2.16.6",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-image-picker": "^2.3.4",
    "react-native-modal-dropdown": "git+https://github.com/siemiatj/react-native-modal-dropdown.git",
    "react-native-reanimated": "^1.13.1",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-vector-icons": "^7.1.0",
    "react-navigation": "^4.4.2",
    "react-test-renderer": "^16.13.1",
    "sonarqube-scanner": "^2.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@react-native-community/eslint-config": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.4.0",
    "@typescript-eslint/parser": "^4.4.0",
    "babel-jest": "^26.5.2",
    "cz-cli": "^1.0.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-plugin-react": "^7.21.3",
    "jest": "^26.5.2",
    "metro-react-native-babel-preset": "^0.63.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "collectCoverageFrom": [
      "scene/**.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/",
      "!<rootDir>/path/to/dir/",
      "!src/serviceWorker.js"
    ],
    "coverageReporters": [
      "text",
      "lcov"
    ],
    "coverageDirectory": "coverage",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.assetstransformer.js",
      "\\.(css|less)$": "<rootDir>/.assetstransformer.js"
    }
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 2
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
